<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0.0">
	<xs:complexType name="ADDRESS_type">
		<xs:sequence>
			<xs:element ref="NAME"/>
			<xs:element ref="NAME2" minOccurs="0"/>
			<xs:element ref="NAME3" minOccurs="0"/>
			<xs:element ref="STREET" minOccurs="0"/>
			<xs:element ref="ZIP" minOccurs="0"/>
			<xs:element ref="BOXNO" minOccurs="0"/>
			<xs:element ref="ZIPBOX" minOccurs="0"/>
			<xs:element ref="CITY" minOccurs="0"/>
			<xs:element name="DISTRICT" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="36"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element ref="STATE" minOccurs="0"/>
			<xs:element ref="COUNTRY" minOccurs="0"/>
			<xs:element ref="PHONE" minOccurs="0"/>
			<xs:element ref="MOBIL" minOccurs="0"/>
			<xs:element ref="FAX" minOccurs="0"/>
			<xs:element ref="EMAIL" minOccurs="0"/>
			<xs:element ref="PUBLIC_KEY" minOccurs="0"/>
			<xs:element ref="URL" minOccurs="0"/>
			<xs:element ref="ADDRESS_REMARKS" minOccurs="0"/>
			<xs:element name="ID_DB" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="type">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="50"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="CONTACT_type">
		<xs:sequence>
			<xs:element name="SALUTATION" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TITLE" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="FIRST_NAME" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LAST_NAME">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="NAME_PREFIX" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="JOB_TITLE" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element ref="PHONE" minOccurs="0"/>
			<xs:element ref="MOBIL" minOccurs="0"/>
			<xs:element ref="FAX" minOccurs="0"/>
			<xs:element ref="EMAIL" minOccurs="0"/>
			<xs:element ref="URL" minOccurs="0"/>
			<xs:element ref="ADDRESS_REMARKS" minOccurs="0"/>
			<xs:element name="ID_DB" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element ref="ADDRESS" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="type">
			<xs:simpleType>
				<xs:restriction base="xs:integer"/>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="ADDRESS" type="ADDRESS_type"/>
	<xs:element name="ADDRESS_REMARKS">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="4000"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="AGREEMENT">
		<xs:annotation>
			<xs:documentation>Informationen zu den Rahmenverträgen, auf deren Basis das
                Katalogdokument erstellt wird</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AGREEMENT_ID"/>
				<xs:element name="VALID_FROM" type="xs:dateTime" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Datum, ab dem der Rahmenvertrag gültig
                        ist</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="VALID_THROGH" type="xs:dateTime" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Datum bis zum dem der Rahmenvertrag gültig
                        ist.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AGREEMENT_ID">
		<xs:annotation>
			<xs:documentation>Informationen zum anbietenden (katalogerzeugenden)
            Unternehmen</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="ALLOWED_VALUE">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ALLOWED_VALUE_ID"/>
				<xs:element ref="ALLOWED_VALUE_NAME"/>
				<xs:element ref="ALLOWED_VALUE_DESCR" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ALLOWED_VALUES">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ALLOWED_VALUE" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ALLOWED_VALUE_DESCR">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="ALLOWED_VALUE_ID">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="60"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="ALLOWED_VALUE_IDREF">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="60"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="ALLOWED_VALUE_NAME">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="60"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="AUTHOR" type="ADDRESS_type"/>
	<xs:element name="BOXNO">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="10"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="RECIPIENT">
		<xs:annotation>
			<xs:documentation>Informationen zum einkaufenden (katalogempfangenden) Unternehmen bzw.
                zum Datenempfänger</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RECIPIENT_ID"/>
				<xs:element ref="RECIPIENT_NAME"/>
				<xs:element ref="ADDRESS"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RECIPIENT_AID">
		<xs:annotation>
			<xs:documentation>Artikelnummer des Katalogempfängers</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="RECIPIENT_ID">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="RECIPIENT_NAME">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CATALOG">
		<xs:annotation>
			<xs:documentation>Informationen zur Identifikation und Beschreibung des Produktkatalogs
                und des Katalogdokuments sowie Bereiche zum Setzen von
            Default-Werten</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="LANGUAGE"/>
				<xs:element ref="CATALOG_ID"/>
				<xs:element ref="CATALOG_VERSION" minOccurs="0"/>
				<xs:element ref="CATALOG_NAME" minOccurs="0"/>
				<xs:element name="GENERATION_DATE" type="xs:dateTime" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Erstellungsdatum des Kataloges</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="TERRITORY" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="CURRENCY" minOccurs="0"/>
				<xs:element ref="MIME_ROOT" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CATALOG_ID">
		<xs:annotation>
			<xs:documentation>ID des Kataloges. Wird vom Katalogerzeuger
            vergeben.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CATALOG_NAME">
		<xs:annotation>
			<xs:documentation>Name des Kataloges</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CATALOG_VERSION">
		<xs:annotation>
			<xs:documentation>Versionsnummer des Kataloges.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CBT_CONTACT" type="ADDRESS_type">
		<xs:annotation>
			<xs:documentation>Kontakt für Fragen bzgl. des CBT.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CITY">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="36"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CLASSIFICATION_GROUP">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CLASSIFICATION_GROUP_ID" minOccurs="0" maxOccurs="5"/>
				<xs:element ref="CLASSIFICATION_GROUP_NAME"/>
				<xs:element ref="CLASSIFICATION_GROUP_DESCR" minOccurs="0"/>
				<xs:element ref="CLASSIFICATION_GROUP_SYNONYMS" minOccurs="0"/>
				<xs:element ref="CLASSIFICATION_GROUP_FEATURE_TEMPLATES" minOccurs="0"/>
				<xs:element ref="CLASSIFICATION_GROUP_PARENT_ID" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="node"/>
						<xs:enumeration value="leaf"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="level" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="CLASSIFICATION_GROUPS">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CLASSIFICATION_GROUP" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CLASSIFICATION_GROUP_DESCR">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CLASSIFICATION_GROUP_FEATURE_TEMPLATE">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FT_IDREF"/>
				<xs:element ref="FT_MANDATORY"/>
				<xs:element ref="FT_DATATYPE"/>
				<xs:element ref="FT_UNIT" minOccurs="0"/>
				<xs:element ref="FT_ORDER" minOccurs="0"/>
				<xs:element ref="FT_ALLOWED_VALUES" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CLASSIFICATION_GROUP_FEATURE_TEMPLATES">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CLASSIFICATION_GROUP_FEATURE_TEMPLATE" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CLASSIFICATION_GROUP_ID">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="60"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CLASSIFICATION_GROUP_NAME">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="60"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CLASSIFICATION_GROUP_PARENT_ID" type="xs:string"/>
	<xs:element name="CLASSIFICATION_GROUP_SYNONYMS">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="SYNONYM" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CLASSIFICATION">
		<xs:annotation>
			<xs:documentation>CLASSIFICATION bildet die Zuordnung zur Systematik
            ab</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CLASSIFICATION_SYSTEM_NAME"/>
				<xs:element ref="CLASSIFICATION_GROUP_ID" maxOccurs="5"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CLASSIFICATION_SYSTEM">
		<xs:annotation>
			<xs:documentation>Die Struktur CLASSIFICATION SYSTEM erlaubt es, mehrsprachige und
                hierarchische Klassifikationssysteme inklusive Synonymen und komplexen
                Merkmalsleisten mit Datentypen und Vorgabewerten zu übertragen.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CLASSIFICATION_SYSTEM_NAME"/>
				<xs:element ref="CLASSIFICATION_SYSTEM_FULLNAME" minOccurs="0"/>
				<xs:element ref="CLASSIFICATION_SYSTEM_VERSION" minOccurs="0"/>
				<xs:element ref="CLASSIFICATION_SYSTEM_DESCR" minOccurs="0"/>
				<xs:element ref="CLASSIFICATION_SYSTEM_LEVELS" minOccurs="0"/>
				<xs:element ref="CLASSIFICATION_SYSTEM_LEVEL_NAMES" minOccurs="0"/>
				<xs:element ref="ALLOWED_VALUES" minOccurs="0"/>
				<xs:element ref="UNITS" minOccurs="0"/>
				<xs:element ref="CLASSIFICATION_SYSTEM_FEATURE_TEMPLATES" minOccurs="0"/>
				<xs:element ref="CLASSIFICATION_GROUPS"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CLASSIFICATION_SYSTEM_DESCR">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CLASSIFICATION_SYSTEM_FEATURE_TEMPLATE">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FT_ID"/>
				<xs:element ref="FT_NAME"/>
				<xs:element ref="FT_DESCR" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CLASSIFICATION_SYSTEM_FEATURE_TEMPLATES">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CLASSIFICATION_SYSTEM_FEATURE_TEMPLATE" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CLASSIFICATION_SYSTEM_FULLNAME">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CLASSIFICATION_SYSTEM_LEVELS" type="xs:integer"/>
	<xs:element name="CLASSIFICATION_SYSTEM_LEVEL_NAME">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="60"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CLASSIFICATION_SYSTEM_LEVEL_NAMES">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CLASSIFICATION_SYSTEM_LEVEL_NAME" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CLASSIFICATION_SYSTEM_NAME">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="20"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CLASSIFICATION_SYSTEM_VERSION">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="10"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
