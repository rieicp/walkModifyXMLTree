	<xs:element name="HEADER">
		<xs:annotation>
			<xs:documentation>Der Kopfteil wird durch das Element HEADER spezifiziert. Im HEADER
                werden allgemeine Informationen über das Katalogdokument hinterlegt und
                Default-Werte gesetzt.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="GENERATOR_INFO" minOccurs="0"/>
				<xs:element ref="CATALOG"/>
				<xs:element name="DOCUMENT_CREATOR" type="typeCONTACT" minOccurs="0">
					<xs:annotation>
						<xs:documentation source="description" xml:lang="de">Katalogerzeuger</xs:documentation>
						<xs:documentation source="descr_short" xml:lang="de">Informationen zum katalogerzeugenden Unternehmen</xs:documentation>
						<xs:documentation source="title" xml:lang="de">In diesem Element werden Informationen zum katalogerzeugenden Unternehmen übertragen</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="RECIPIENT" minOccurs="0"/>
				<xs:element ref="AGREEMENT" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="SUPPLIER"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="INDUSTRY_GROUP_ID">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="60"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="INDUSTRY_GROUP_NAME">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="60"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="INSTRUCTOR">
		<xs:annotation>
			<xs:documentation>Referent(en)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="INSTRUCTOR_SERVICETIME">
		<xs:annotation>
			<xs:documentation>Der Zeitraum innerhalb dessen ein Trainer für den WBT zur Verfügung
                steht.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="START_DATE"/>
				<xs:element ref="END_DATE" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ISBN">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="ISSN">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="KEYWORD">
		<xs:annotation>
			<xs:documentation>Schlagwort, welches das Finden des Produktes bzw. Anbieters
                erleichtert; das Produkt soll im Zielsystem auch über die Eingabe dieses
                Schlagwortes gefunden werden können - Später Anzahl begrenzen!!</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="LANGUAGE">
		<xs:annotation>
			<xs:documentation>Sprache des Kataloges</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="LICENCE">
		<xs:annotation>
			<xs:documentation>Lizenzbedingungen für die Nutzung des CBTs.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="64000"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="LOCATION" type="ADDRESS_type">
		<xs:annotation>
			<xs:documentation>Ort der Veranstaltung</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="LOWER_BOUND" type="xs:float"/>
	<xs:element name="MANDATORY_LEARNINGTIME">
		<xs:annotation>
			<xs:documentation>Ein Zeitraum innerhalb dessen ein Lernender zwingend am WBT arbeiten
                muss. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="START_DATE"/>
				<xs:element ref="END_DATE"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="MARKETINGTEXT">
		<xs:annotation>
			<xs:documentation>Mit Hilfe von MARKETINGTEXT können Angaben zum Inhalt bzw. zum
                Programm der Veranstaltung angegeben werden. Dies kann auch detaillierter über die
                Verwendung von Modulen gemacht werden </xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="64000"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="MAX_PARTICIPANTS" type="xs:integer">
		<xs:annotation>
			<xs:documentation>Höchstzahl Teilnehmer, die eine Veranstaltung besuchen
            können.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="MEDIA">
		<xs:annotation>
			<xs:documentation>Die in der Weiterbildungsmaßnahme verwendeten Medien. Beispiel:
                Übungsunterlagen, Filmvortrag</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="METHOD">
		<xs:annotation>
			<xs:documentation>Die in der Weiterbildungsmaßnahme angewandten Methoden können mittels
                dieses Elementes übertragen werden. Beispiel: Gruppenarbeit, Rollenspiel,
            Diskussion</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="MIMEElement">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="MIME_TYPE" minOccurs="0"/>
				<xs:element ref="MIME_SOURCE"/>
				<xs:element ref="MIME_DESCR" minOccurs="0"/>
				<xs:element ref="MIME_ALT" minOccurs="0"/>
				<xs:element ref="MIME_PURPOSE" minOccurs="0"/>
				<xs:element ref="MIME_ORDER" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="MIME_ALT">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="MIME_DESCR">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="64000"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="MIME_INFO">
		<xs:annotation>
			<xs:documentation>Hier können multimediale Zusatzinformationen (z.B. Bilder) übertragen
                werden.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="MIMEElement" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="MIME_ORDER" type="xs:integer"/>
	<xs:element name="MIME_PURPOSE">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="64000"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="MIME_ROOT">
		<xs:annotation>
			<xs:documentation>Wurzelverzeichnis/URL in dem MIME Erweiterungen gesucht werden
            sollen.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="MIME_SOURCE">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="2000"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="MIME_TYPE">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="MIN_PARTICIPANTS" type="xs:integer">
		<xs:annotation>
			<xs:documentation>Mindestzahl Teilnehmer, die für eine Durchführung der Veranstaltung
                nötig ist.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="MODULE_CBT">
		<xs:annotation>
			<xs:documentation>Mit dieser Struktur können Informationen zu Computer-gestützten
                Lernformen übertragen werden.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="MARKETINGTEXT" minOccurs="0"/>
				<xs:element ref="AUTHOR" minOccurs="0"/>
				<xs:element ref="CBT_CONTACT" minOccurs="0"/>
				<xs:element ref="LICENCE" minOccurs="0"/>
				<xs:element ref="EST_DURATION" minOccurs="0"/>
				<xs:element ref="ISBN" minOccurs="0"/>
				<xs:element ref="ISSN" minOccurs="0"/>
				<xs:element ref="TECH_REQUIREMENTS_MIN" minOccurs="0"/>
				<xs:element ref="TECH_REQUIREMENTS_RECOMMENDED" minOccurs="0"/>
				<xs:element ref="SERVICE_REFERENCE" minOccurs="0"/>
				<xs:element ref="MODULE_ORDER" minOccurs="0"/>
				<xs:element ref="METHOD" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MEDIA" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MIME_INFO" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="MODULE_COURSE">
		<xs:annotation>
			<xs:documentation>Mit dieser Struktur können Informationen zu klassischen
                Präsenzveranstaltungen übertragen werden.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="MARKETINGTEXT" minOccurs="0"/>
				<xs:element ref="INSTRUCTOR" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="LOCATION" type="ADDRESS_type" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Ort, an dem die Veranstaltung stattfindet. Wenn der
                            Veranstaltungsort bei Katalogerstellung noch nicht feststeht, kann diese
                            Angabe auch weggelassen werden.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="START_DATE" minOccurs="0"/>
				<xs:element ref="END_DATE" minOccurs="0"/>
				<xs:element ref="MODULE_ORDER" minOccurs="0"/>
				<xs:element ref="METHOD" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MEDIA" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MIME_INFO" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="segment_type" type="xs:integer"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="MODULE_ORDER" type="xs:integer">
		<xs:annotation>
			<xs:documentation>Reihenfolge, in der die Module im Zielsystem dargestellt werden In
                Listendarstellungen von Modulen werden die Module in aufsteigender Reihenfolge
                dargestellt (erstes Modul entspricht niedrigster Zahl).</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="MODULE_ORGANIZER" type="ADDRESS_type">
		<xs:annotation>
			<xs:documentation>Organisator bzw. Durchführende Institution: Falls die ausführende bzw.
                organisierende Institution vor der Katalogerzeugenden Organisation abweicht, können
                diese Angaben durch die Struktur MODULE_ORGANIZIER übermittelt werden.
            </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="MODULE_WBT">
		<xs:annotation>
			<xs:documentation>Mit dieser Struktur kann eine Aus- und Weiterbildungsdienstleistung
                von Typ „Web Based Training“ abgebildet werden.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="MARKETINGTEXT" minOccurs="0"/>
				<xs:element ref="AUTHOR" minOccurs="0"/>
				<xs:element ref="WBT_CONTACT" minOccurs="0"/>
				<xs:element ref="SERVICETIME" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="INSTRUCTOR_SERVICETIME" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MANDATORY_LEARNINGTIME" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="LICENCE" minOccurs="0"/>
				<xs:element ref="EST_DURATION" minOccurs="0"/>
				<xs:element ref="TECH_REQUIREMENTS_MIN" minOccurs="0"/>
				<xs:element ref="TECH_REQUIREMENTS_RECOMMENDED" minOccurs="0"/>
				<xs:element ref="SERVICE_REFERENCE" minOccurs="0"/>
				<xs:element ref="MODULE_ORDER" minOccurs="0"/>
				<xs:element ref="METHOD" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MEDIA" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MIME_INFO" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="NAME">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="30"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="NAME2">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="30"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="NAME3">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="30"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="NAME4">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="30"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="OPENQCAT">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="HEADER"/>
				<xs:element ref="NEW_CATALOG"/>
				<xs:element ref="UPDATE_CATALOG" minOccurs="0" maxOccurs="1"/>
			</xs:sequence>
			<xs:attribute name="version" type="xs:string" fixed="0.9.2"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="PHONE">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="30"/>
				<xs:pattern value="\+*[0-9]+\.[0-9]+\.{0,1}[0-9]*"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="MOBIL">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="30"/>
				<xs:pattern value="\+*[0-9]+\.[0-9]+\.{0,1}[0-9]*"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="PRICE_AMOUNT" type="xs:float"/>
	<xs:element name="PRICE_CURRENCY" type="CURRENCY_TYPE"/>
	<xs:element name="PRICE_FACTOR" type="xs:float"/>
	<xs:element name="PUBLIC_KEY">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="64000"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="REFERENCE_DESCRIPTION">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="64000"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="REFERENCE_FEATURE_GROUP_ID">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="60"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="REFERENCE_FEATURE_GROUP_NAME">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="REFERENCE_FEATURE_SYSTEM_NAME">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="REQ_CPU">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="120"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="REQ_GRAPHICSCARD">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="120"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="REQ_HARDDRIVE">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="120"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="REQ_MEMORY">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="120"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="REQ_OPERATINGSYSTEM">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="120"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="REQ_OTHERHARDWARE">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="120"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="REQ_OTHERSOFTWARE">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="120"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="SERVICE">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PRODUCT_ID"/>
				<xs:element ref="COURSE_TYPE"/>
				<xs:element ref="CLASSIFICATION"/>
				<xs:element ref="SERVICE_DETAILS"/>
				<xs:element ref="SERVICE_MODULE" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="HS_INFO" minOccurs="0"/>
				<xs:element ref="MIME_INFO" minOccurs="0"/>
				<xs:element ref="SERVICE_REFERENCE" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="SERVICE_FEATURES" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="mode" type="xs:string" fixed="new"/>
			<xs:attribute name="rejected" type="xs:boolean" use="optional" default="false"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="SERVICE_MODULE">
		<xs:annotation>
			<xs:documentation>alle dienstleistungsspezifischen Informationen der Aus- und
                Weiterbildungsdienstleistungen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="COURSE_ID" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Nummer des Kurses bzw. Verweis auf den zugehörigen
                        Kurs.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="INSTRUCTION_FORM" type="xs:integer">
					<xs:annotation>
						<xs:documentation>Unterrichtsform (z.B. Vollzeit, Teilzeit, Blockuntericht
                            usw.)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="SERVICE_DATE" minOccurs="0"/>
				<xs:element ref="DURATION" minOccurs="0"/>
				<xs:element ref="LOCATION"/>
				<xs:element ref="CONTACT" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="REGISTRATION_DATE" type="xs:dateTime" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Spätest mögliches Anmeldedatum</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="INSTRUCTION_REMARKS" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Bemerkung zu Unterrichtsform und
                        -zeiten</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="2000"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="MIN_PARTICIPANTS" minOccurs="0"/>
				<xs:element ref="MAX_PARTICIPANTS" minOccurs="0"/>
				<xs:element name="EXAMINER" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Prüfende Stelle</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="2000"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="SERVICE_PRICE_DETAILS" minOccurs="0"/>
				<xs:element name="TERMS_AND_CONDITIONS" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Allgemeine Regelungen, die für das Angebot
                        gelten.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="8000"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="MIME_INFO" minOccurs="0"/>
				<xs:element name="GOV_METADATA" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Für BA notwendige Daten</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="MEASURE_NR" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Maßnahmenummer</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="20"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="ACCOUNT_NUMBER" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Kontonummer</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
