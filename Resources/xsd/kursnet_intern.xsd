	<xs:complexType name="typeSTUDY_COURSE">
		<xs:sequence>
			<xs:element name="DEGREE_INFO" type="typeDEGREE_INFO" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Abschluss relevante Informationen, z.B. Mastertyp, Doppelabschluss usw.</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Abschluss relevante Informationen, z.B. Mastertyp, Doppelabschluss usw.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="STUDY_TYPE" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Studienformen (z.B. Ausbildungs-integrierend)</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Die Zuordnung zu einer Studienform erfolgt über das Attribut 'type': Für die korrekte Einordnung in KURSNET ist die Angabe einer Studienform mittels der zur Studienform gehörigen ganzen Zahl erforderlich. Der für open-Qcat in der vorliegenden Version gültigen Wertebereich ist der KURSNET-Website zu entnehmen.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="STUDY_TYPE_ADD_INFO" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Ergänzende Informationen zur Studienform</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Ergänzende Informationen zur Studienform wird als Freitext übertragen.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1500"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="REGULAR_STUDY_PERIOD" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Regelstudienzeit</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Regelstudienzeit wird als Anzahl (aktuell zwischen 1 und 20) vom Typ REGULAR_STUDY_PERIOD_TYPE übertragen.</xs:documentation>
				</xs:annotation>
  				<xs:simpleType>
    				<xs:restriction base="xs:integer">
      					<xs:minInclusive value="1"/>
      					<xs:maxInclusive value="20"/>
    				</xs:restriction>
  				</xs:simpleType>
			</xs:element>
			<xs:element name="REGULAR_STUDY_PERIOD_TYPE" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Typ für Regelstudienzeit</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Die Regelstudienzeit ist von der angegebene Art (entweder Semester oder Trimester). Eine Nicht-Angabe wird als Default-Typ Semester ausgewertet.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:whiteSpace value="collapse"/>
						<xs:enumeration value="semester"/>
						<xs:enumeration value="trimester"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ACCREDITATION" type="typeACCREDITATION" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Akkreditierung</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Informationen zur Akkreditierung</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EDU_RESTRICTIONS" type="typeEDU_RESTRICTIONS" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Bedingungen zum Studienangebot</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Bedingungen zum Studienangebot</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SPECIALIZATION" type="typeSPECIALIZATION" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Studien-Schwerpunkte</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Studien-Schwerpunkte</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="typeDEGREE_INFO">
		<xs:sequence>
			<xs:element name="DEGREE_TYPE" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Abschlusstyp (z.B. Mastertyp)</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Abschlusstyp (z.B. Mastertyp). Die Zuordnung zu einem Abschlusstyp erfolgt über das Attribut 'type': Für die korrekte Einordnung in KURSNET ist die Angabe eines Abschlussyps mittels der zum Abschlusstyp gehörigen ganzen Zahl erforderlich. Der für open-Qcat in der vorliegenden Version gültigen Wertebereich ist der KURSNET-Website zu entnehmen.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="UNIV_DEGREE" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Abschlussgrad (z.B. Diplom)</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Die Zuordnung zu einem Abschlussgrad erfolgt über das Attribut 'type': Für die korrekte Einordnung in KURSNET ist die Angabe eines Abschlussgrades mittels der zum Abschlussgrad gehörigen ganzen Zahl erforderlich. Der für open-Qcat in der vorliegenden Version gültigen Wertebereich ist der KURSNET-Website zu entnehmen.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TEACHING_QUALIFICATION" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Kennzeichnung, ob der Studiengang einen zum Lehramt befähigt</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Studiengang ist Lehramtsbefähigend</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TEACHING_DEGREE" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Abschluss: Lehramt nach Schulformen</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Die Zuordnung zu einem Lehramt erfolgt über das Attribut 'type': Für die korrekte Einordnung in KURSNET ist die Angabe eines Lehramtes mittels der zum Abschluss gehörigen ganzen Zahl erforderlich. Der für open-Qcat in der vorliegenden Version gültigen Wertebereich ist der KURSNET-Website zu entnehmen.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DOUBLE_DEGREE" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Internationaler Doppelabschluss (Double Degree)</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">True: Doppelabschluss möglich, False: Doppelabschluss nicht möglich</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="INTERNAL_DEGREE" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Abschlussgrad int.</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Geben Sie hier den von der Hochschule vergebenen Abschlussgrad ein. (z.B.:  Bachelor of Arts, Bachelor of Engineering, Bachelor of Fine Arts, Master of Science, Artist Diploma, Meisterklassenexamen).</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="120"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="typeACCREDITATION">
		<xs:sequence>
			<xs:element name="TYPE" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Erstakkreditierung oder Reakkreditierung</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Erstakkreditierung oder Reakkreditierung. Die Zuordnung zu einer Akkreditierung erfolgt über das Attribut 'type': Für die korrekte Einordnung in KURSNET ist die Angabe einer Akkreditierung mittels der zur Akkreditierung gehörigen ganzen Zahl erforderlich. Der für open-Qcat in der vorliegenden Version gültigen Wertebereich ist der KURSNET-Website zu entnehmen.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="FROM" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Akkreditierung von Datum (Format: format YYYY-MM-DD)</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Akkreditierung von Datum (Format: format YYYY-MM-DD)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TO" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Akkreditierung bis Datum (Format: format YYYY-MM-DD)</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Akkreditierung bis Datum (Format: format YYYY-MM-DD)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CONDITIONS" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Auflagen zur Akkreditierung</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Auflagen zur Akkreditierung werden als Freitext übertragen.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="typeEDU_RESTRICTIONS">
		<xs:sequence>
			<xs:element name="SSC_REQUIRED" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Studieren ohne Abitur möglich oder nicht möglich</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">False: Studieren ohne Abitur nicht möglich, True: Studieren ohne Abitur möglich</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WO_SSC" type="typeWO_SSC" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Bedingungen zum Studienangebot</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Bedingungen zum Studienangebot</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SSC_DESC" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Studieren ohne Abitur: Weitere Informationen</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Weitere Informationen werden als Freitext übertragen.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="typeWO_SSC">
		<xs:sequence>
			<xs:element name="CONDITIONS_WO_SSC" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Studieren ohne Abitur: Zugangsbedingungen</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Pflichtfeld, wenn Studieren ohne Abitur möglich ist (SSC_REQUIRED = False). Weitere Informationen zu Zugangsbedingungen werden als Freitext (CONDITIONS_WO_SSC_DESC) übertragen.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CONDITIONS_WO_SSC_DESC" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Studieren ohne Abitur: Weitere Informationen zu Zugangsbedingungen</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Weitere Informationen zu Zugangsbedingungen werden als Freitext übertragen.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="typeSPECIALIZATION">
		<xs:sequence>
			<xs:element name="CORE_AREA" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="description" xml:lang="de">Studien-Schwerpunkte (z.B. Tourismusmanagement)</xs:documentation>
					<xs:documentation source="descr_short" xml:lang="de">Studien-Schwerpunkte werden als Freitext übertragen</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
