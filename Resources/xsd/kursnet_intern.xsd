	<xs:element name="DEGREE">
		<xs:annotation>
			<xs:documentation>Abschlusszertifikat / Zeugnis: Falls die Aus- und
                Weiterbildungsmassnahme ein Anschlusszertifikat, eine Berechtigung oder ein
                Abschlusszeugnis vorsieht, kann dieses in der Struktur DEGREE modelliert werden.
                Attribut Type gibt die Art des Abschlusses/Berechtigung an.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="DEGREE_TITLE" minOccurs="0"/>
				<xs:element ref="DEGREE_EXAM" minOccurs="0"/>
				<xs:element name="DEGREE_ADD_QUALIFICATION" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="4000"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="DEGREE_ENTITLED" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="4000"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="type" use="optional">
				<xs:annotation>
					<xs:documentation>Art des Abschlusses</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Öffentlich anerkannt"/>
						<xs:enumeration value="Befähigungsnachweis"/>
						<xs:enumeration value="Industriezertifikat"/>
						<xs:enumeration value="Anbieterspezifisch"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="DEGREE_EXAM">
		<xs:annotation>
			<xs:documentation>Abschlussprüfung</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="EXAM_DESCRIPTION" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Beschreibung</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="64000"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="LOCATION" minOccurs="0"/>
				<xs:element ref="EXAM_DATE" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="EXAM_LEGAL_BASIS" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Rechtsgrundlage</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="64000"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="type" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Art der Abschlußprüfung</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="DEGREE_TITLE">
		<xs:annotation>
			<xs:documentation>Bezeichnung/Beschreibung des Abschlusses</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="64000"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="TIME_TO_HANDLE" type="xs:unsignedShort">
		<xs:annotation>
			<xs:documentation>Zeit in Werktagen, die der Anbieter zur Bereitstellung der
                Dienstleistung bzw. Lieferung des Produktes benötigt
            (Vorlaufzeit)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="DESCRIPTION_LONG">
		<xs:annotation>
			<xs:documentation>Langbeschreibung des Produktes; Formatierung mit
            HTML-Tags</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="30000"/>
				<!--				
<xs:minLength value="1"/>
-->
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="TITLE">
		<xs:annotation>
			<xs:documentation>Kurzbeschreibung</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="0"/>
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="DURATION">
		<xs:annotation>
			<xs:documentation>Dauer der Maßnahme in Stunden, Tagen oder Einteilung in "Dauerklassen"
                (type attribut)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="type" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:integer"/>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="EMAIL">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="END_DATE" type="xs:dateTime"/>
	<xs:element name="EST_DURATION">
		<xs:annotation>
			<xs:documentation>Die geschätzte Bearbeitungsdauer in Zeiteinhalten. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:float">
					<xs:attribute name="type" type="xs:string"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="EXAM_DATE" type="xs:dateTime">
		<xs:annotation>
			<xs:documentation>Mit EXAM_DATE kann das Datum bzw. die Dauer der Abschlussprüfung
                übertragen werden. Sollte die Prüfung an mehreren Tagen stattfinden, so können
                mehrere EXAM_DATE Elemente verwendet werden.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="FAX">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="30"/>
				<xs:pattern value="\+*[0-9]+\.[0-9]+\.{0,1}[0-9]*"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="FDESCR">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
