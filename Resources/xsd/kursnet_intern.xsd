								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="64000"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="HANDICAP" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Behinderungen</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="HAND_TYPE" type="xs:integer" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Art der Behinderung</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="HAND_NAME" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Name der Behinderung</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="255"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="HAND_DESCRIPTION" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Beschreibung</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="64000"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="ORDER" type="xs:integer" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Sortierreihenfolge</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="HAND_CATEGORY" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Behinderungsklasse</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="1"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="SERVICE_ORDER" minOccurs="0"/>
				<xs:element ref="PRODUCT_ALT_ID" minOccurs="0"/>
				<xs:element ref="RECIPIENT_AID" minOccurs="0"/>
				<xs:element ref="SERVICE_STATUS" minOccurs="0"/>
				<xs:element ref="TIME_TO_HANDLE" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SERVICE_FEATURES">
		<xs:annotation>
			<xs:documentation>In diesem Element kann der Artikel klassifiziert und mit Merkmalen
                beschrieben werden und/oder die Artikelvarianten angegeben werden.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="REFERENCE_FEATURE_SYSTEM_NAME" minOccurs="0"/>
				<xs:choice minOccurs="0">
					<xs:element ref="REFERENCE_FEATURE_GROUP_ID"/>
					<xs:element ref="REFERENCE_FEATURE_GROUP_NAME"/>
				</xs:choice>
				<xs:element ref="FEATURE" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SERVICE_ID_TO">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="SERVICE_ORDER" type="xs:integer">
		<xs:annotation>
			<xs:documentation>Reihenfolge, in der Produkte im Zielsystem dargestellt
            werden</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="SERVICE_PRICE">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PRICE_AMOUNT"/>
				<xs:element ref="PRICE_CURRENCY" minOccurs="0"/>
				<xs:element ref="TAX" minOccurs="0"/>
				<xs:element ref="PRICE_FACTOR" minOccurs="0"/>
				<xs:element ref="LOWER_BOUND" minOccurs="0"/>
				<xs:element ref="TERRITORY" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="price_type" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="SERVICE_PRICE_DETAILS">
		<xs:annotation>
			<xs:documentation>Mit dem Element SERVICE_PRICE_DETAILS werden Preisdaten eines Artikels
                spezifiziert.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="VALID_FROM" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Zeitpunkt ab dem der Preis gilt.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="DATE"/>
							<xs:element ref="TIME" minOccurs="0"/>
							<xs:element ref="TIMEZONE" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="DAILY_PRICE" minOccurs="0"/>
				<xs:element ref="SERVICE_PRICE" minOccurs="0"/>
				<xs:element name="REMARKS" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Bemerkung zu den Preisen</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="2000"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SERVICE_REFERENCE">
		<xs:annotation>
			<xs:documentation>Über die Produktstrukturen ist es möglich, auf andere Module und/oder
                Dienstleistungen zu verweisen. Diese Verweise haben dabei eine feste Bedeutung, sie
                legen also fest, in welcher Beziehung die beiden Produkte zueinander stehen. Ein
                solcher Verweis kann z.B. von einem Weiterbildungsangebot auf ein anderes
                Weiterbildungsangebot verweisen, dessen Inhalte als Voraussetzungen
            dienen.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="SERVICE_ID_TO"/>
				<xs:element ref="CATALOG_ID" minOccurs="0"/>
				<xs:element ref="CATALOG_VERSION" minOccurs="0"/>
				<xs:element ref="REFERENCE_DESCRIPTION" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="type" use="required">
				<xs:annotation>
					<xs:documentation>Die Verweisart beschreibt, in welcher Beziehung die beiden
                        Produkte zueinander stehen (z.B. Artikel zu Ersatzteil). </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="sparepart"/>
						<xs:pattern value="similar"/>
						<xs:pattern value="followup"/>
						<xs:pattern value="mandatory"/>
						<xs:pattern value="select"/>
						<xs:pattern value="accessories"/>
						<xs:pattern value="consists_of"/>
						<xs:pattern value="constitutive"/>
						<xs:pattern value="predecessor"/>
						<xs:pattern value="others"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="SERVICE_STATUS">
		<xs:annotation>
			<xs:documentation>Die Art des Status wird im Attribut "type" spezifiziert. Der Wert des
                Elements nimmt die textuelle Beschreibung des Status auf. Es ist so möglich, eine
                Dienstleistung beispielsweise als Sonderangebot oder als neues Produkt zu
                kennzeichnen und zu kommentieren.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="START_DATE" type="xs:dateTime"/>
	<xs:element name="STATE">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="80"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="STREET">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="30"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="SUPPLIER">
		<xs:annotation>
			<xs:documentation>Informationen zum anbietenden (katalogerzeugenden)
            Unternehmen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="SUPPLIER_ID" maxOccurs="unbounded"/>
				<xs:element name="EXT_ID" minOccurs="0">
					<xs:annotation>
						<xs:documentation>ID des Bildungsanbieters (Fremdsystem)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="SUPPLIER_NAME"/>
				<xs:element ref="ADDRESS"/>
				<xs:element name="CONTACT" type="CONTACT_type" maxOccurs="unbounded"/>
				<xs:element name="INSTITUTION_NR" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Trägernummer</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="8"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="FUNDING_BODY" minOccurs="0">
					<xs:annotation>
						<xs:documentation source="description" xml:lang="de">Trägerschaft der Hochschule</xs:documentation>
						<xs:documentation source="descr_short" xml:lang="de">Trägerschaft der Hochschule. Die Zuordnung zu einer Trägerschaft der Hochschule erfolgt über das Attribut 'type': Für die korrekte Einordnung in KURSNET ist die Angabe einer Trägerschaft mittels der zur Trägerschaft gehörigen ganzen Zahl erforderlich. Der für open-Qcat in der vorliegenden Version gültigen Wertebereich ist der KURSNET-Website zu entnehmen.
						</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:integer"/>
					</xs:simpleType>
				</xs:element>
				<xs:element name="ORGANIZATIONAL_FORM" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Organisationsform</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="4"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="STANDARD_COMPANY_NUMBER" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Betriebsnummer</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="8"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CUSTOMER_NUMBER" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Kundennummer (Zebra)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="20"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="ACCOUNT_NUMBER" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Kontonummer</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="15"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="BANK_CODE" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Bankleitzahl</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="10"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="KEYWORD" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MIME_INFO" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="input" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:integer"/>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="PRODUCT_ID">
		<xs:annotation>
			<xs:documentation>eindeutige Artikelnummer des Anbieters</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="COURSE_TYPE">
		<xs:annotation>
			<xs:documentation>Angebotstyp des Kurses</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:int">
				<xs:enumeration value="1">
					<xs:annotation>
						<xs:documentation source="description" xml:lang="de">Bildungsangebot</xs:documentation>
						<xs:documentation source="descr_short" xml:lang="de">Bildungsangebot</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="2">
					<xs:annotation>
						<xs:documentation source="description" xml:lang="de">Studienangebot</xs:documentation>
						<xs:documentation source="descr_short" xml:lang="de">Studienangebot</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="3">
					<xs:annotation>
						<xs:documentation source="description" xml:lang="de">Maßnahmeangebot AVGS</xs:documentation>
						<xs:documentation source="descr_short" xml:lang="de">Maßnahmeangebot AVGS</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="PRODUCT_ALT_ID">
		<xs:annotation>
			<xs:documentation>Alternative Artikelnummer</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="SUPPLIER_ID">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="type" use="optional" default="company_number">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="duns"/>
								<xs:enumeration value="iln"/>
								<xs:enumeration value="buyer_specific"/>
								<xs:enumeration value="supplier_specific"/>
								<xs:enumeration value="company_number"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="SUPPLIER_NAME">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="SYNONYM">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="60"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="TARGET_GROUP">
		<xs:annotation>
			<xs:documentation>TARGET_GROUP definiert eine Zielgruppe, an die sich die angebotene
                Dienstleistung richtet.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="TARGET_GROUP_TEXT" minOccurs="0"/>
				<xs:element ref="TARGET_GROUP_HIERARCHY" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TARGET_GROUP_INDUSTRY" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TARGET_GROUP_DEPARTMENT" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TARGET_GROUP_DEPARTMENT">
		<xs:annotation>
			<xs:documentation>Mit diesem Element kann die Zielgruppe über die Abteilung bzw. den
                Fachbereich spezifiziert werden.</xs:documentation>
			<xs:documentation/>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="60"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="TARGET_GROUP_HIERARCHY">
		<xs:annotation>
			<xs:documentation>Mit diesem Element kann die Zielgruppe über die Hierarchieebene
                spezifiziert werden.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="60"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="TARGET_GROUP_INDUSTRY">
		<xs:annotation>
			<xs:documentation>Mit diesem Element kann die Zielgruppe über die Branche spezifiziert
                werden.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="INDUSTRY_GROUP_ID" minOccurs="0"/>
				<xs:element ref="INDUSTRY_GROUP_NAME" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
						<xs:enumeration value="WZ93"/>
						<xs:enumeration value="NACE"/>
						<xs:enumeration value="ISIC"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="version" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
		<!--			<xs:attribute name="type" type="xs:string" use="required"> -->
	</xs:element>
	<xs:element name="TARGET_GROUP_TEXT">
		<xs:annotation>
			<xs:documentation>In diesem Element kann die Zielgruppe für eine Veranstaltung als
                Freitext übermittelt werden.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="64000"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="TAX" type="xs:float"/>
	<xs:element name="TECH_REQUIREMENTS_MIN">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="REQ_CPU" minOccurs="0"/>
				<xs:element ref="REQ_MEMORY" minOccurs="0"/>
				<xs:element ref="REQ_HARDDRIVE" minOccurs="0"/>
				<xs:element ref="REQ_GRAPHICSCARD" minOccurs="0"/>
				<xs:element ref="REQ_OPERATINGSYSTEM" minOccurs="0"/>
				<xs:element ref="REQ_OTHERSOFTWARE" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="REQ_OTHERHARDWARE" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TECH_REQUIREMENTS_RECOMMENDED">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="REQ_CPU" minOccurs="0"/>
				<xs:element ref="REQ_MEMORY" minOccurs="0"/>
				<xs:element ref="REQ_HARDDRIVE" minOccurs="0"/>
				<xs:element ref="REQ_GRAPHICSCARD" minOccurs="0"/>
				<xs:element ref="REQ_OPERATINGSYSTEM" minOccurs="0"/>
				<xs:element ref="REQ_OTHERSOFTWARE" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="REQ_OTHERHARDWARE" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TERRITORY">
		<xs:annotation>
			<xs:documentation>Regionale Gültigkeiten</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="TIME" type="xs:time"/>
	<xs:element name="TIMEZONE" type="xs:string"/>
	<!-- risk -->
	<xs:element name="UPDATE_CATALOG">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="DELETE" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="SERVICE" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="PRODUCT_ID">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="255"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="NEW" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="SERVICE" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- risk -->
	<xs:element name="NEW_CATALOG">
		<xs:annotation>
			<xs:documentation>Die Struktur NEW_CATALOG enthält die
            Dienstleistungsinformationen.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CLASSIFICATION_SYSTEM" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="SERVICE" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="FULLCATALOG" type="xs:boolean" use="optional" default="1"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="UNIT">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="UNIT_ID"/>
				<xs:element ref="UNIT_NAME" minOccurs="0"/>
				<xs:element ref="UNIT_DESCR" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="system" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="UNITS">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="UNIT" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="UNIT_DESCR">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="UNIT_ID">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="60"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="UNIT_NAME">
		<xs:simpleType>
