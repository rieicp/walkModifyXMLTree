					<xsd:documentation source="descr_short" xml:lang="de">Mindestzahl Teilnehmer, die für eine Durchführung der Veranstaltung notwendig ist.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MAX_PARTICIPANTS" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
