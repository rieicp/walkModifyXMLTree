	<xsd:complexType name="typeMIME_INFO">
		<xsd:sequence>
			<xsd:element name="MIME_ELEMENT" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="de">Multmediales Dokument</xsd:documentation>
					<xsd:documentation source="descr_short" xml:lang="de">Das Element MIME dient zur Übertragung von Informationen zu jeweils einer multimedialen Datei. Die Datei selbst wird nur referenziert und muss separat übertragen werden.</xsd:documentation>
					<xsd:documentation source="title" xml:lang="de">Das Element MIME dient zur Übertragung von Informationen zu jeweils einer multimedialen Datei. Die Datei selbst wird nur referenziert und muss separat übertragen werden.</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="MIME_TYPE" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="de">Mime-Typ</xsd:documentation>
								<xsd:documentation source="descr_short" xml:lang="de">Typ des Zusatzdokumentes; dieses Element orientiert sich an den im Internet gebräuchlichen Mimetypes (ftp://ftp.isi.edu/in-notes/rfc1341.txt)</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="255"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="MIME_SOURCE">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="de">Quelle</xsd:documentation>
								<xsd:documentation source="descr_short" xml:lang="de">Der relative Pfad und der Dateiname bzw. die URL-Adresse, der MIME_SOURCE-String wird mit dem Basispfad (<link>MIME_ROOT</link>) aus dem <link>HEADER</link> kombiniert (daran angehängt mittels einfacher Konkatenation) Unterverzeichnisse sind mit "Slashes" ("/") voneinander zu trennen (z.B. /public/document/demo.pdf).</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="255"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="MIME_DESCR" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="de">Beschreibung des Anhangs</xsd:documentation>
								<xsd:documentation source="descr_short" xml:lang="de">Beschreibung der Zusatzdatei; diese Beschreibung soll im Zielsystem angezeigt werden</xsd:documentation>
								<xsd:documentation source="title" xml:lang="de">Beschreibung der Zusatzdatei; diese Beschreibung soll im Zielsystem angezeigt werden</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="64000"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="MIME_ALT" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="de">Alternativtext</xsd:documentation>
								<xsd:documentation source="descr_short" xml:lang="de">Alternativtext, falls die Datei zum Beispiel in dem Zielsystem nicht darstellbar ist</xsd:documentation>
								<xsd:documentation source="title" xml:lang="de">Alternativtext, falls die Datei zum Beispiel in dem Zielsystem nicht darstellbar ist</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="255"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="MIME_PURPOSE" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="de">Zweck</xsd:documentation>
								<xsd:documentation source="descr_short" xml:lang="de">Gewünschter Verwendungszweck des MIME-Dokumentes</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:minLength value="1"/>
									<xsd:maxLength value="20"/>
									<xsd:enumeration value="data_sheet">
										<xsd:annotation>
											<xsd:documentation source="description" xml:lang="de">Produktdatenblatt</xsd:documentation>
											<xsd:documentation source="descr_short" xml:lang="de">Produktdatenblatt</xsd:documentation>
										</xsd:annotation>
									</xsd:enumeration>
									<xsd:enumeration value="detail">
										<xsd:annotation>
											<xsd:documentation source="description" xml:lang="de">Vergrößerungsansicht</xsd:documentation>
											<xsd:documentation source="descr_short" xml:lang="de">vergrößertes Bild</xsd:documentation>
										</xsd:annotation>
									</xsd:enumeration>
									<xsd:enumeration value="icon">
										<xsd:annotation>
											<xsd:documentation source="description" xml:lang="de">Icon</xsd:documentation>
											<xsd:documentation source="descr_short" xml:lang="de">Kleines Piktogramm z.B. zur Angabe erfüllter Normen oder Standards</xsd:documentation>
										</xsd:annotation>
									</xsd:enumeration>
									<xsd:enumeration value="logo">
										<xsd:annotation>
											<xsd:documentation source="description" xml:lang="de">Logo</xsd:documentation>
											<xsd:documentation source="descr_short" xml:lang="de">Logo zu einem Produkt oder einem Anbieteren; siehe auch Element <link>SUPPLIER</link> im <link>HEADER</link>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:enumeration>
									<xsd:enumeration value="normal">
										<xsd:annotation>
											<xsd:documentation source="description" xml:lang="de">Normalansicht</xsd:documentation>
											<xsd:documentation source="descr_short" xml:lang="de">Normalbild (normal)</xsd:documentation>
										</xsd:annotation>
									</xsd:enumeration>
									<xsd:enumeration value="thumbnail">
										<xsd:annotation>
											<xsd:documentation source="description" xml:lang="de">Kleinansicht</xsd:documentation>
											<xsd:documentation source="descr_short" xml:lang="de">Vorschaubild (klein)</xsd:documentation>
										</xsd:annotation>
									</xsd:enumeration>
									<xsd:enumeration value="osa">
										<xsd:annotation>
											<xsd:documentation source="description" xml:lang="de">Link OSA</xsd:documentation>
											<xsd:documentation source="descr_short" xml:lang="de">Link zum Online Self Assessment (OSA)</xsd:documentation>
										</xsd:annotation>
									</xsd:enumeration>
									<xsd:enumeration value="others">
										<xsd:annotation>
											<xsd:documentation source="description" xml:lang="de">Sonstiges</xsd:documentation>
											<xsd:documentation source="descr_short" xml:lang="de">Sonstiger Verwendungszweck. Sollte keiner der anderen Werte passen, kann others verwendet werden.</xsd:documentation>
										</xsd:annotation>
									</xsd:enumeration>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="MIME_ORDER" type="xsd:integer" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="de">Darstellungsreihenfolge</xsd:documentation>
								<xsd:documentation source="descr_short" xml:lang="de">Einfache Reihenfolge, in der Zusatzdaten im Zielsystem dargestellt werden sollen. In Listendarstellungen von Zusatzdokumenten sollen die Dokumente in aufsteigender Reihenfolge dargestellt werden (erstes Dokument entspricht niedrigster Zahl)</xsd:documentation>
								<xsd:documentation source="title" xml:lang="de">Einfache Reihenfolge, in der Zusatzdaten im Zielsystem dargestellt werden sollen. In Listendarstellungen von Zusatzdokumenten sollen die Dokumente in aufsteigender Reihenfolge	dargestellt werden (erstes Dokument entspricht niedrigster Zahl)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="typeMODULE_CBT">
		<xsd:sequence>
			<xsd:element name="MARKETINGTEXT" type="typeMARKETINGTEXT" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="de">Vertriebstext</xsd:documentation>
					<xsd:documentation source="descr_short" xml:lang="de">Mit Hilfe von MARKETINGTEXT können Angaben zum Inhalt bzw. zum Programm des CBT angegeben werden.</xsd:documentation>
					<xsd:documentation source="title" xml:lang="de">Mit Hilfe von MARKETINGTEXT können Angaben zum Inhalt bzw. zum Programm des CBT angegeben werden. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
