					<xsd:documentation source="description" xml:lang="de">Kontaktinformationen</xsd:documentation>
					<xsd:documentation source="descr_short" xml:lang="de">Kontakt für Fragen bzgl. des CBT.</xsd:documentation>
					<xsd:documentation source="title" xml:lang="de">Kontakt für Fragen bzgl. des CBT.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LICENCE" type="typeLICENCE" minOccurs="0">
